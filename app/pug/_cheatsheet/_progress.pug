//- Progress
//-=========================================================================

h1.cheatsheet Progress
.row

		.col-xl-6
			p.cheatsheet normal bar

			// normal bar
			.progress
				.progress-bar(role="progressbar", style="width: 25%", aria-valuenow="25", aria-valuemin="0", aria-valuemax="100")


		.col-xl-6
			p.cheatsheet bar striped, animated

			// bar striped, animated
			.progress
				.progress-bar.progress-bar-striped.progress-bar-animated.bg-success(role="progressbar", aria-valuenow="75", aria-valuemin="0", aria-valuemax="100", style="width: 75%")
